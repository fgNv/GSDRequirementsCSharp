<h3>Vínculos</h3>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Item</th>
            <th>Tipo do item</th>
            <th>Criador</th>
            <th>Data</th>
        </tr>
    </thead>
    <tbody>
        <tr data-ng-repeat="link in links">
            <td>{{link.Item}}</td>
            <td>{{link.ItemType}}</td>
            <td>{{link.Creator}}</td>
            <td>{{link.CreationDate | date:'dd/MM/yyyy'}}</td>
            <td></td>
        </tr>
    </tbody>
</table>
